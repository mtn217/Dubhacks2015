<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton13"
        android:layout_gravity="left|top"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="150dp"
        android:background="@mipmap/profilefemale"
        android:clickable="true" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton18"
        android:layout_gravity="left|center_vertical"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="-60dp"
        android:background="@mipmap/trending" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton19"
        android:layout_gravity="left|bottom"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="20dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton20"
        android:layout_gravity="right|top"
        android:layout_marginTop="150dp"
        android:layout_marginRight="20dp"
        android:background="@mipmap/mycloset" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton21"
        android:layout_gravity="right|center_vertical"
        android:layout_marginRight="20dp"
        android:layout_marginTop="60dp"
        android:background="@mipmap/weathersunny" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton22"
        android:layout_gravity="right|bottom"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="30dp"
        android:background="@mipmap/Cortana" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="mylook"
        android:id="@+id/textView9"
        android:layout_gravity="center_horizontal|top"
        android:layout_marginTop="30dp" />
</FrameLayout>